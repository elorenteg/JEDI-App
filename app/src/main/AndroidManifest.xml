<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.esterlorente.jediapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/GreenAppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".CalculadoraActivity" />
        <activity
            android:name=".MemoryActivity"
            android:label="@string/title_activity_memory" />
        <activity
            android:name=".LogedActivity"
            android:label="@string/title_activity_loged"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".GameActivity"
            android:label="@string/title_activity_game"></activity>
    </application>

</manifest>
